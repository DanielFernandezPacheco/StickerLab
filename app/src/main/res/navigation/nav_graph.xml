<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/CategoriesFragment">

    <fragment
        android:id="@+id/CategoriesFragment"
        android:name="es.fdi.stickerlab.CategoriesFragment"
        tools:layout="@layout/fragment_categories">

        <action
            android:id="@+id/action_CategoriesFragment_to_makerFragment"
            app:destination="@id/makerFragment" />
    </fragment>
    <fragment
        android:id="@+id/makerFragment"
        android:name="es.fdi.stickerlab.MakerFragment"
        android:label="fragment_maker"
        tools:layout="@layout/fragment_maker" >
        <action
            android:id="@+id/action_makerFragment_to_CategoriesFragment"
            app:destination="@id/CategoriesFragment" />
    </fragment>

</navigation>